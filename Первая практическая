list_1 = [1,2,3,4,5,3,6,7,2,8,] #создаём список

print(list_1[-2]) #выводим предпоследний элемент
print(list_1[0]) #выводим первый элемент
print(list_1[1]) # выводим второй элемент

list_1[3] = 2 #меняем четвёртое значение на 2

print(list_1[0:4]) #выводим список в определённом диапазоне


list_1.append(9) #добавляем в конец списка значение
print(list_1)

list_1.insert(5,11) #добавляем в заданную позицию значение
print(list_1)

print(list_1.count(2)) #считаем количество орпеделённого значения

list_2 = list_1.copy() #копируем список
list_1[2] = 1 
print(list_1)
print(list_2)


